pipeline {
    agent any

    stages {
        stage('build') {
            steps {
                echo 'building the application'
            }
        }
        stage('test') {
            steps {
                echo 'testing the application'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploying the application'
                echo 'your pipeline is successful'
            }
        }

    }
}
